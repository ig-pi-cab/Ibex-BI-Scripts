FROM
  `hvr-dwh-dev-437001.dataset_bronze.tesmov_bronze`
WHERE
  AUDIT_INSERT_DTTM >= TIMESTAMP_ADD(CURRENT_TIMESTAMP(), INTERVAL -31 DAY)
