  SII_ControlCesion_RUT_TENEDOR,
  SAFE_CAST(SII_ControlCesion_LOG_FECHA AS DATETIME) AS SII_ControlCesion_LOG_FECHA,
  SII_ControlCesion_CLAVESESION,
